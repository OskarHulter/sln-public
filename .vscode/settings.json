{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontSize": 18,
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "workbench.colorCustomizations": {},
  "WillLuke.nextjs.addTypesOnSave": false,
  "WillLuke.nextjs.hasPrompted": true,
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.exclude": {
    "**/.babelrc*": true,
    "**/.DS_Store": true,
    "**/.editorconfig": true,
    "**/.eslintignore": true,
    "**/.eslintrc*": true,
    "**/.git": true,
    "**/.gitattributes": true,
    "**/.github": true,
    "**/.gitignore": true,
    "**/.gitmodules": true,
    "**/.hg": true,
    "**/.next": true,
    "**/.npmrc": true,
    "**/.svn": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/.yarnrc*": true,
    "**/build": true,
    "**/CVS": true,
    "**/dist": true,
    "**/jsconfig.json": true,
    "**/LICENSE": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/README.*": true,
    "**/Thumbs.db": true,
    "**/yarn.lock": true
  },
  "explorer.excludeGitIgnore": true
}
