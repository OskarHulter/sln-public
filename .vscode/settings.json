{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontSize": 18,
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "workbench.colorCustomizations": {},
  "WillLuke.nextjs.addTypesOnSave": false,
  "WillLuke.nextjs.hasPrompted": true,
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/.babelrc*": false,
    "**/.editorconfig": false,
    "**/.eslintignore": false,
    "**/.eslintrc*": false,
    "**/.gitattributes": false,
    "**/.github": false,
    "**/.gitignore": false,
    "**/.gitmodules": false,
    "**/.next": false,
    "**/.npmrc": false,
    "**/.vscode": false,
    "**/.yarn": false,
    "**/.yarnrc*": false,
    "**/build": false,
    "**/dist": false,
    "**/jsconfig.json": false,
    "**/LICENSE": false,
    "**/node_modules": false,
    "**/package-lock.json": false,
    "**/pnpm-lock.yaml": false,
    "**/README.*": false,
    "**/yarn.lock": false
  },
  "explorer.excludeGitIgnore": true,
  "explorerExclude.backup": {}
}
