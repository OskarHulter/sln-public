{
  "name": "data-access-shared",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/data-access-shared/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/data-access-shared",
        "tsConfig": "packages/data-access-shared/tsconfig.lib.json",
        "packageJson": "packages/data-access-shared/package.json",
        "main": "packages/data-access-shared/src/index.ts",
        "assets": [
          "packages/data-access-shared/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/data-access-shared/**/*.ts"
        ]
      }
    }
  },
  "tags": [
    "scope:shared",
    "type:util"
  ]
}
