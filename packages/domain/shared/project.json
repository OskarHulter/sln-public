{
  "name": "domain-shared",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/domain/shared/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/domain/shared"
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": [
        "coverage/packages/domain/shared"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/packages/domain/shared"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/domain/shared/**/*.ts"
        ]
      }
    }
  },
  "tags": [
    "scope:shared",
    "type:feature"
  ]
}
